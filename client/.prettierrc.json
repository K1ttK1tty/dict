{
    "semi": true,
    "tabWidth": 4,
    "singleQuote": true,
    "printWidth": 120,
    "importOrder": [
        "<THIRD_PARTY_MODULES>",
        "(components/*)|(UI/)|(App/)",
        "(functions/*)|(hooks/*)",
        "(styles/*)|(.css)",
        "/globalConsts*",
        "/store*",
        "/Others*",
        "(models/*)|(.functoinModels.*) |(Model)",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "arrowParens": "avoid"
}
